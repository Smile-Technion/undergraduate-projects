		</body>
		
</worldbody>
	
	<actuator>
		<motor joint='a'/>
		<motor joint='b'/>
		<motor joint='c'/>
	</actuator>	
	
	<sensor>
			<velocimeter name="tar_vel" site="target"/>
			<accelerometer  name="tar_acc" site="target"/>
			<force name="tar_touch" site="target"/>
			<torque name="tar_torque" site="target"/>
			
	</sensor>

